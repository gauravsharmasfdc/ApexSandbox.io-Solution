public Boolean isPrime(Integer num) {
     if (number <= 1) {
        return false;
    }
    if (number == 2) {
        return true; 
    }
    if (number % 2 == 0) {
        return false; 
    }

    Integer sqrt = Math.sqrt(number).intValue();
    for (Integer i = 3; i <= sqrt; i += 2) {
        if (Math.mod(number,i) == 0) {
            return false;
        }
    }

    return true; 
}