public List<Integer> positiveIntegers(List<Integer> numbers) {
    List<Integer> nonPositiveNumbers = new List<Integer>();
    if(numbers == null || numbers.isEmpty()) return null;

    numbers.sort();
    Integer SIZE = numbers.size();
    for(Integer i = 0 ; i<= SIZE; i++){
        if(numbers[i]<=0){
            nonPositiveNumbers.add(numbers[i]);
        }else{
            break;
        }
    }

    return nonPositiveNumbers;
}