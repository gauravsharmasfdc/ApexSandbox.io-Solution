public List<Integer> positiveIntegers(List<Integer> numbers) {
    List<Integer> postiveNumber = new List<Integer>();
    if(numbers == null || numbers.isEmpty()) return postiveNumber;

    Integer SIZE = numbers.size();
    for(Integer i = 0 ; i< SIZE; i++){
        if(numbers[i]>0){
            postiveNumber.add(numbers[i]);
        }
    }

    return postiveNumber;
}