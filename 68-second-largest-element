public Integer secondLargest(List<Integer> numbers) {

    numbers.sort();
    Integer largest = numbers[0];
    Integer secLargest = numbers[0];

    for(Integer num : numbers){
        if(num > largest){
            secLargest = largest;
            largest = num;
        } else if(num > secLargest && num < largest){
            secLargest = num;
        }
    }
    return secLargest;
}