public Integer secondLargest(List<Integer> numbers) {
    Integer largest = null;
    Integer secLargest = null;

    for(Integer num : numbers){
        if(largest == null || num > largest){
            secLargest = largest;
            largest = num;
        } else if(largest == null || (num > secLargest && num < largest)){
            secLargest = num;
        }
    }
    return secLargest;
}