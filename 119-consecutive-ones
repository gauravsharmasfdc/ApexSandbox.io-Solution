public Integer maxConsecutiveOnes(Integer[] numbers) {
        Integer currentCount=0 ;
        Integer maxCount=0;
        
        for(Integer num : numbers){
            if(num == 1){
                currentCount++;
            }else {
                maxCount = Math.max(maxCount,currentCount);
                currentCount= 0;
            }
        }
        
        return maxCount;
    }