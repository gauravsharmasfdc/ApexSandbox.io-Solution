public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    list<sObject> sObjectList = new List<sObject>();
    if(accounts.isEmpty() && contacts.isEmpty()) return sObjectList;

    if(!accounts.isEmpty()) sObjectList.addAll(accounts);
    if(!contacts.isEmpty()) sObjectList.addAll(contacts);

    return sObjectList;
}