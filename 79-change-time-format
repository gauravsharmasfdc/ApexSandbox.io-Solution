public String changeTimeFormat(String strTime) {
    string hours = strTime.subStringBefore(':');
    String minutes = strTime.subStringAfter(':');
    Integer hoursInt = Integer.valueOf(hours);

    if(hoursInt == 12){
        return String.valueOf(hoursInt) +':'+ minutes+' PM';
    }
    if(hoursInt > 12){
        hoursInt= hoursInt- 12;
        if(hoursInt < 10){
            hours = '0'+String.valueOf(hoursInt)
        }
        return hours +':'+ minutes+' PM';
    }
    
    return  String.valueOf(hoursInt) +':'+ minutes+' AM';

        
}