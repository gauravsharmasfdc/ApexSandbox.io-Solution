public Boolean sameElements(List<Integer> nums1, List<Integer> nums2) {
    Set<Integer> allElements = new Set<Integer>();
    allElements.addAll(nums1);
    allElements.addAll(nums2);

    for(Integer num : allElements){
        if(!nums1.contains(num) || !nums2.contains(num)) { return false;}
    }
    return true;
}