public class LinkedList{
    private Node head;
    private Node tail;
    private Integer size;

    public LinkedList(){
        this.head = null;
        this.tail = null;
        this.size = 0;
    }
    
    public void addToFront(Integer value){
        Node newNode = new Node(value);
        if(this.head == null){
            this.head = newNode;
            this.tail = this.head;
            this.size++;
            return;
        }

        newNode.next = this.head;
        this.head = newNode;
        this.size++;
    }
    public Integer removeFromFront(){
        Integer removedItem;
        if(this.head == null){
            return null;
        }
        Node newHead = this.head.next;
        this.head.next = null;
        removedItem = this.head.data;
        this.head = newHead;
        this.size--;
        return removedItem;
    }
    public void addToTail(Integer value){
        Node newNode = new Node(value);
        if(this.head == null){
            this.head = newNode;
            this.tail = this.head;
            this.size++;
            return;
        }
        this.tail.next = newNode;
        this.tail = newNode;
        this.size++;
    }
    public Integer removeFromTail(){
        Integer removedItem;
        if(this.head == null){
            return null;
        }

        Node newTail = this.head;
        while(newTail.next != this.tail){
            newTail = newTail.next;
        }

        newTail.next = null;
        removedItem = this.tail.data;
        this.tail = newTail;
        this.size--;
        return removedItem;
    }
    public Integer size(){
        return this.size;
    }    
}

public class Node{
    Integer data;
    Node next;

    Node(Integer data){
        this.data = data;
        this.next = null;
    }
}