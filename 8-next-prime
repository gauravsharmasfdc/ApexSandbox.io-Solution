public Integer nextPrime(Integer num) {
    
    Integer primeNum = num+1;
    boolean primeFound = false;
    while(!primeFound){
        if (primeNum <= 1) {
            primeFound = false;
        } else if(primeNum == 2)
            primeFound = true;
        else if (primeNum % 2 == 0) {
            primeFound = false; 
        } else {
            for (Integer i = 3; i <= Math.sqrt(primeNum); i += 2) {
                if (primeNum % i == 0) {
                    primeFound = false; 
                }
            }
        }
        primeNum++;
    }

    return primeNum;


}