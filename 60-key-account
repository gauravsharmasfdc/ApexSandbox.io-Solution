public Boolean isKeyAccount(Account a) {
    private Map<String,Decimal> industryToRevenue  = new Map<String,Decimal>{
        'Banking' : 600000,
        'Technology' : 800000,
        'Retail' : 2000000
    }

    return (
            (industryToRevenue.containsKey(a.Industry) && a.AnnualRevenue >= industryToRevenue.get(a.Industry)) ||
            (!industryToRevenue.containsKey(a.Industry) && a.AnnualRevenue >= 500000)
        );
}